{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "explorer",
    "version": "0.1.0",
    "identifier": "com.explorer.app",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "devUrl": "http://localhost:1420",
        "frontendDist": "../build"
    },
    "app": {
        "windows": [
        {
            "title": "explorer",
            "width": 800,
            "height": 600,
            "dragDropEnabled": true,
            "transparent": false
        }
        ],
        "security": {
        "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
        ],
        "resources": [
            "C:/Users/levin/OneDrive/Code/tauri_app/explorer/static/favicon.png"
        ]
    }
}
